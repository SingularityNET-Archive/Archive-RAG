<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns
                             http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">
  <!-- Extended GraphML for dynamic/temporal analysis -->
  <!-- Created: 2025-09-22 -->
  <!-- Node attributes -->
  <key id="k_type" for="node" attr.name="type" attr.type="string"/>
  <key id="k_stake" for="node" attr.name="stake" attr.type="double"/>
  <key id="k_first_seen" for="node" attr.name="first_seen" attr.type="string"/>
  <!-- SPO-specific node attributes -->
  <key id="k_pool_fee" for="node" attr.name="pool_fee" attr.type="double"/>
  <key id="k_pledge" for="node" attr.name="pledge" attr.type="double"/>
  <key id="k_blocks_minted" for="node" attr.name="blocks_minted" attr.type="int"/>

  <!-- Edge attributes -->
  <key id="k_relation" for="edge" attr.name="relation" attr.type="string"/>
  <key id="k_weight" for="edge" attr.name="weight" attr.type="double"/>
  <!-- Temporal attributes: ISO-8601 and epoch numbers -->
  <key id="k_start_time" for="edge" attr.name="start_time" attr.type="string"/>
  <key id="k_end_time" for="edge" attr.name="end_time" attr.type="string"/>
  <key id="k_start_epoch" for="edge" attr.name="start_epoch" attr.type="int"/>
  <key id="k_end_epoch" for="edge" attr.name="end_epoch" attr.type="int"/>

  <graph id="CardanoEcosystemDynamic" edgedefault="directed">
    <!-- Nodes -->
    <node id="dA">
      <data key="k_type">delegator</data>
      <data key="k_stake">1200.0</data>
      <data key="k_first_seen">2024-11-05T00:00:00Z</data>
    </node>
    <node id="dB">
      <data key="k_type">delegator</data>
      <data key="k_stake">800.0</data>
      <data key="k_first_seen">2025-01-12T00:00:00Z</data>
    </node>
    <node id="dC">
      <data key="k_type">delegator</data>
      <data key="k_stake">500.0</data>
      <data key="k_first_seen">2025-03-20T00:00:00Z</data>
    </node>

    <node id="s1">
      <data key="k_type">SPO</data>
      <data key="k_stake">5000.0</data>
      <data key="k_pool_fee">0.025</data>
      <data key="k_pledge">150000.0</data>
      <data key="k_blocks_minted">812</data>
      <data key="k_first_seen">2019-06-10T00:00:00Z</data>
    </node>
    <node id="s2">
      <data key="k_type">SPO</data>
      <data key="k_stake">3000.0</data>
      <data key="k_pool_fee">0.035</data>
      <data key="k_pledge">50000.0</data>
      <data key="k_blocks_minted">410</data>
      <data key="k_first_seen">2020-09-02T00:00:00Z</data>
    </node>

    <node id="gX">
      <data key="k_type">governance_rep</data>
      <data key="k_stake">2500.0</data>
      <data key="k_first_seen">2021-04-15T00:00:00Z</data>
    </node>
    <node id="gY">
      <data key="k_type">governance_rep</data>
      <data key="k_stake">700.0</data>
      <data key="k_first_seen">2022-08-01T00:00:00Z</data>
    </node>

    <node id="p1">
      <data key="k_type">proposal</data>
      <data key="k_stake">0.0</data>
      <data key="k_first_seen">2025-08-15T00:00:00Z</data>
    </node>
    <node id="p2">
      <data key="k_type">proposal</data>
      <data key="k_stake">0.0</data>
      <data key="k_first_seen">2025-09-01T00:00:00Z</data>
    </node>

    <!-- Edges: include temporal start/end and epoch numbers as samples -->
    <!-- Delegation edges -->
    <edge id="e1" source="dA" target="s1">
      <data key="k_relation">delegation</data>
      <data key="k_weight">1200.0</data>
      <data key="k_start_time">2025-07-01T00:00:00Z</data>
      <data key="k_end_time">2025-09-22T00:00:00Z</data>
      <data key="k_start_epoch">444</data>
      <data key="k_end_epoch">454</data>
    </edge>

    <edge id="e2" source="dB" target="s1">
      <data key="k_relation">delegation</data>
      <data key="k_weight">800.0</data>
      <data key="k_start_time">2025-06-15T00:00:00Z</data>
      <data key="k_end_time">2026-01-01T00:00:00Z</data>
      <data key="k_start_epoch">441</data>
      <data key="k_end_epoch">490</data>
    </edge>

    <edge id="e3" source="dC" target="s2">
      <data key="k_relation">delegation</data>
      <data key="k_weight">500.0</data>
      <data key="k_start_time">2025-05-20T00:00:00Z</data>
      <data key="k_end_time">2025-09-22T00:00:00Z</data>
      <data key="k_start_epoch">435</data>
      <data key="k_end_epoch">454</data>
    </edge>

    <!-- Governance delegation edges -->
    <edge id="e4" source="dA" target="gX">
      <data key="k_relation">gov_delegation</data>
      <data key="k_weight">1200.0</data>
      <data key="k_start_time">2025-07-01T00:00:00Z</data>
      <data key="k_end_time">2026-07-01T00:00:00Z</data>
      <data key="k_start_epoch">444</data>
      <data key="k_end_epoch">548</data>
    </edge>

    <edge id="e5" source="dB" target="gX">
      <data key="k_relation">gov_delegation</data>
      <data key="k_weight">800.0</data>
      <data key="k_start_time">2025-06-20T00:00:00Z</data>
      <data key="k_end_time">2026-06-20T00:00:00Z</data>
      <data key="k_start_epoch">442</data>
      <data key="k_end_epoch">547</data>
    </edge>

    <edge id="e6" source="dC" target="gY">
      <data key="k_relation">gov_delegation</data>
      <data key="k_weight">500.0</data>
      <data key="k_start_time">2025-03-21T00:00:00Z</data>
      <data key="k_end_time">2025-12-31T00:00:00Z</data>
      <data key="k_start_epoch">425</data>
      <data key="k_end_epoch">485</data>
    </edge>

    <!-- Governance vote edges (temporal: vote date) -->
    <edge id="e7" source="gX" target="p1">
      <data key="k_relation">vote</data>
      <data key="k_weight">1.0</data>
      <data key="k_start_time">2025-08-20T12:00:00Z</data>
      <data key="k_end_time">2025-08-20T12:00:00Z</data>
      <data key="k_start_epoch">448</data>
      <data key="k_end_epoch">448</data>
    </edge>

    <edge id="e8" source="gY" target="p2">
      <data key="k_relation">vote</data>
      <data key="k_weight">1.0</data>
      <data key="k_start_time">2025-09-05T15:30:00Z</data>
      <data key="k_end_time">2025-09-05T15:30:00Z</data>
      <data key="k_start_epoch">452</data>
      <data key="k_end_epoch">452</data>
    </edge>

    <!-- Optional: pool cooperation or performance-change signal edges -->
    <edge id="e9" source="s1" target="s2">
      <data key="k_relation">pool_cooperation</data>
      <data key="k_weight">0.0</data>
      <data key="k_start_time">2024-12-01T00:00:00Z</data>
      <data key="k_end_time">2025-12-01T00:00:00Z</data>
      <data key="k_start_epoch">400</data>
      <data key="k_end_epoch">520</data>
    </edge>

  </graph>
</graphml>
